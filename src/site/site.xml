<project xmlns="http://maven.apache.org/DECORATION/1.3.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd"
	name="${project.name} ${project.version}">
	<publishDate position="left" format="dd-MMMM-yyyy" />
	<version position="left" />
	<skin>
		<groupId>org.apache.maven.skins</groupId>
		<artifactId>maven-fluido-skin</artifactId>
		<version>1.3.1</version>
	</skin>
	<bannerLeft>
		<name>${project.organization.name}</name>
		<src>images/logo.png</src>
		<href>${project.organization.url}</href>
	</bannerLeft>
	<bannerRight>
		<name>${project.name}</name>
		<href>${project.url}</href>
	</bannerRight>

	<body>
		<head>
			<link rel="shortcut icon" href="favicon.ico" type="image/x-icon" />
			<link rel="icon" href="favicon.ico" type="image/x-icon" />
			<link rel="meta" title="DOAP"
				href="${project.url}/doap_${project.artifactId}.rdf" type="application/rdf+xml" />
		</head>
		<links>
			<item name="Issues dashboard" href="${project.issueManagement.url}" />
		</links>
		<menu name="Information">
			<item name="Home" href="/index.html" />
            <item name="Usage" href="/plugin-info.html" />
            <item name="FAQ" href="/faq.html" />
			<item name="Release notes" href="/releasenotes.html" />
			<item name="Developer Notes" href="/development.html" />
		</menu>
		<menu ref="parent" />
		<menu ref="modules" />
		<menu ref="reports" />
	</body>
	<poweredBy>
		<logo href="https://travis-ci.org/GeoDienstenCentrum/sass-maven-plugin"
			img="https://travis-ci.org/GeoDienstenCentrum/sass-maven-plugin.svg?branch=master"
			name="Travis-CI Build status" />
		<logo href="https://ci.appveyor.com/project/mprins/sass-maven-plugin"
			img="https://ci.appveyor.com/api/projects/status/qxc604b7o0rwxsel?svg=true"
			name="AppVeyor-CI Build status" />
		<logo href="https://coveralls.io/r/GeoDienstenCentrum/sass-maven-plugin"
			img="https://img.shields.io/coveralls/GeoDienstenCentrum/sass-maven-plugin.svg"
			name="Coverage status" />
		<logo href="https://maven-badges.herokuapp.com/maven-central/${project.groupId}/${project.artifactId}/"
			img="https://maven-badges.herokuapp.com/maven-central/${project.groupId}/${project.artifactId}/badge.svg"
              name="Maven Central" />
        <logo href="http://sass-lang.com/" width="120px"
              img="images/sass.png"
              name="Sass" />
	</poweredBy>
</project>
